"use strict";if(window["ping_apply.wabash.edu"]==null){window["ping_apply.wabash.edu"]=true;var ping_service="https://apply.wabash.edu/ping?cmd=register",ping_whoami="https://apply.wabash.edu/ping?cmd=whoami";var ping_start=new Date().getTime();window.addEventListener("unload",function(b){var d=new Date().getTime()-ping_start;var g=ping_service+"&d="+encodeURIComponent(d.toString())+"&t="+new Date().getTime().toString();try{if(navigator.sendBeacon){navigator.sendBeacon(g,"")}else{var a=new XMLHttpRequest();a.open("GET",g,false);a.send()}}catch(c){var e=document.getElementsByTagName("SCRIPT")[0];var f=document.createElement("SCRIPT");f.async=true;f.src=g;e.parentNode.insertBefore(f,e)}});if(window.ping_whoami!=null){var s=document.getElementsByTagName("SCRIPT")[0];var s2=document.createElement("SCRIPT");s2.async=true;s2.src=ping_whoami;s.parentNode.insertBefore(s2,s)}};